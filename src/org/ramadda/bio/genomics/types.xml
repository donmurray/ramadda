<types>

  <type name="bio_genomics"  description="Genomics Data" foruser="false"  category="Genomics">
     <property name="icon" value="/genomics/dna.png"/>
     <property name="form.area.show" value="false"/>
     <property name="form.todate.show" value="false"/>
     <property name="form.fromdate.label" value="Date"/>
  </type>

  <type name="bio_genomics_fasta"  description="FASTA File" foruser="true" super="bio_genomics" handler="org.ramadda.bio.genomics.FastaTypeHandler" pattern=".*\.fa$">
  </type>

  <type name="bio_genomics_sequence"  description="Sequence Data" foruser="false" super="bio_genomics">
  </type>

  <type name="bio_genomics_sambam"  description="SAM/BAM Data" foruser="false" super="bio_genomics_sequence" handler="org.ramadda.bio.genomics.SamTypeHandler">
  </type>


  <type name="bio_genomics_sam"  description="SAM Data" foruser="true" super="bio_genomics_sambam">
  </type>

  <type name="bio_genomics_bam"  description="BAM Data" foruser="true" super="bio_genomics_sambam">
  </type>


  <type name="bio_genomics_stockholm"  description="Stockholm File" foruser="true" super="bio_genomics" handler="org.ramadda.bio.genomics.StockholmTypeHandler" pattern=".*\.sto$">
  </type>


<!--
     <column name="data_type" label="Earth Science Data Type" type="enumerationplus" values="" cansearch="true" />
-->


  <type name="bio_genomics_hmmer_index"  description="HMMER Index File" foruser="true" super="bio_genomics" xxxhandler="org.ramadda.bio.genomics.StockholmTypeHandler" pattern=".*\.hmm$">
     <property name="icon" value="/genomics/hmmer.png"/>
  </type>



</types>
