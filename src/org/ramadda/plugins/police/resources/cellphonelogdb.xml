<tables>

<table id="zzz_case_cellphonelog" name="Cell Phone Log" category="Police Case" icon="/case/phone.png" handler="org.ramadda.plugins.police.CellPhoneDbTypeHandler">
   <column name="from_number" type="string" label="From Number"   cansearch="true" canlist="true" editable="false"/>
   <column name="to_number" type="string" label="To Number"   cansearch="true" canlist="true" editable="false"/>
   <column name="time" type="datetime"   label="Date"  cansearch="true" canlist="true" editable="false">
       <property name="label" value="true"/>
   </column>
   <column name="duration" type="double"   label="Duration"  cansearch="true" canlist="true" editable="false"/>
   <column name="type" type="enumeration"   label="Direction" values="inbound:In Bound,outbound:Outbound,voice:Voice Mail,mobiletomobile:Mobile to mobile" cansearch="true" canlist="true" editable="false">
       <property name="iscategory" value="true"/>
   </column>
   <column name="location" type="latlon"   label="Location"  cansearch="true" canlist="false" editable="false"/>
   <column name="tower_address" type="string"   label="Address"  cansearch="false" canlist="false" editable="false"/>
   <column name="tower_city"    type="string"   label="City"  cansearch="true" canlist="true" editable="false"/>
   <column name="tower_state"    type="enumeration"   label="State"  cansearch="true" canlist="true" editable="false" columns="5" values="file:/org/ramadda/plugins/police/resources/states.txt"/>
   <column name="tower_zipcode"    type="string"   label="Zip Code"  cansearch="true" canlist="false" editable="false" columns="10"/>

   <column name="tag"    type="enumerationplus"   label="Tag"  cansearch="true" canlist="true" values="default,important">
       <property name="iscategory" value="true"/>
   </column>
   <column name="notes"    type="string"   label="Notes"  cansearch="true" canlist="true" rows="5"/>
</table>



</tables>
