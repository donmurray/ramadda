     <wiki><![CDATA[
+section # label="Socrata Site: {{name}}"
<br>

{{description wikify="true"}}

{{tree details="false"}}


-section

]]>

</wiki>

  </type>



  <type name="type_socrata_view"  description="Socrata View"  foruser="false" xxxhandler="org.ramadda.plugins.biz.FredSeriesTypeHandler" mefirst="true">
