                properties="/org/ramadda/geodata/cdmdata/resources/level3radar.product.properties"/>
</type>


<type name="cdm_radar_level2" description="Level 2 Radar File" super="cdm_radar_base" handler="org.ramadda.geodata.cdmdata.Level2RadarTypeHandler">
