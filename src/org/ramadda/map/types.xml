   </type>

  <type name="map.entry.polygon"  super="map.entry" description="Map Polygon" handler="org.ramadda.map.MapEntryTypeHandler" foruser="false" >
     <property name="icon" value="/icons/map.png"/>
   </type>
-->

  <type name="map.entry.marker"  super="map.entry" description="Map Marker" handler="org.ramadda.map.MapEntryTypeHandler" foruser="false" >
     <property name="icon" value="/icons/map.png"/>
   </type>

</types>
