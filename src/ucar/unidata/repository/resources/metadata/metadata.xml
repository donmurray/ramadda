<?xml version="1.0" encoding="ISO-8859-1"?>
<metadata>

  <handler category="Tags">


    <type name="Tag" id="enum_tag" searchable="true" browsable="true">
      <element name="Tag Value" />
      <template type="thredds"><![CDATA[<keyword>${attr1}</keyword>]]></template>
      <template type="oaidc"><![CDATA[<dc:subject>${attr1.cdata}</dc:subject>]]></template>
    </type>

    <type name="Wiki Category" id="wikicategory" foruser="false">
      <element name="Wiki Category" />
    </type>


  </handler>


  <handler class="ucar.unidata.repository.metadata.ContentMetadataHandler"        category="Attachments, Contact, Etc.">
    <type
       name="Thumbnail Image"
       id="content.thumbnail">
      <element name="Thumbnail File" datatype="file" thumbnail="true"/>
    </type>

    <type
       name="File Attachment"
       id="content.attachment">
      <element name="File" datatype="file" thumbnail="false"/>
    </type>

    <type
       name="Logo"
       id="content.logo">
      <element name="Logo Image File" datatype="file" thumbnail="false" attachment="false"/>
      <element name="Logo URL"/>
      <element name="Page Title"/>
    </type>

    <type
       name="Jython Script"
       id="content.jython">
      <element name="Jython File" datatype="file" thumbnail="false"/>
    </type>

    <type name="Contact" id="content.contact">
      <element name="Name" />
      <element name="Email" />
    </type>

    <type name="Display Timezone" id="content.timezone">
      <element
         default="GMT"
         name="Timezone"/>
    </type>


    <type name="Biography" id="content.biography"       displaycategory="Biography">

      <element  name="Name"/>
      <element
         default=""
         name="Bio"
	 rows="10"/>
    </type>


    <type name="Sort Order" id="content.sort">
      <element
         default="fromdate"
         name="Field"
         datatype="enumeration"
         values="fromdate:From Date,todate:To Date,createdate:Create Date,name:Name"/>
      <element
         default="true"
         name="Ascending"
         datatype="boolean"/>
<!--
      <element
         default="100"
         name="Max Count"
         datatype="int"/>
-->
    </type>
    <type
       name="Alias"
       id="content.alias">
      <element name="Alias" />
      <template type="html"><![CDATA[<a href="${root}/alias/${attr1}">${attr1}</a>]]></template>
    </type>
  </handler>

  <handler adminonly="true" class="ucar.unidata.repository.metadata.AdminMetadataHandler" category="Admin">
    <type name="Page Template" id="admin.template"/>
    <type name="Local File Pattern" id="admin.localfile.pattern"/>
    <type name="Anonymous Upload" id="admin.anonymousupload">
      <element name="From" />
      <element name="IP" />
    </type>
  </handler>

  <handler class="ucar.unidata.repository.metadata.JpegMetadataHandler"    >
    <type
       name="Camera direction"
       id="camera.direction">
      <element name="Direction" datatype="double" suffix="Degrees"/>
    </type>
  </handler>


  <handler class="ucar.unidata.repository.metadata.MetadataHandler"        category="Miscellaneous">
    <type
       name="Contact Information"
       id="misc.contact">
      <element name="type"/>
      <element name="value"/>
    </type>

    <type
       name="Named Spatial Region"
       id="spatial.region">
      <element name="value"/>
    </type>

    <type
       name="Spatial KML Polygon Coordinates"
       id="spatial.kmlpolycoords">
      <element name="value"/>
    </type>

</handler>



</metadata>
